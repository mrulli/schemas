{
    "@context":
    {
      "schema": "https://schema.org/",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
      "owl": "http://www.w3.org/2002/07/owl#",
      "xsd": "http://www.w3.org/2001/XMLSchema#"
    },
    "@graph":[
      {
        "@id": "name",
        "@type": "rdfs:Property",
        "rdfs:label": "Name",
        "rdfs:subClassOf":
        {
            "@id": "schema:Thing"
        }
      },
      {
        "@id": "description",
        "@type": "rdfs:Property",
        "rdfs:label": "description",
        "rdfs:subClassOf":
        {
            "@id": "schema:Thing"
        }
      },
      {
        "@id": "Resource",
        "@type": "rdfs:Class",
        "rdfs:comment": "Any object that generates data and optionally accepts command.",
        "rdfs:label": "Resource",
        "rdfs:subClassOf":
        {
            "@id": "schema:Thing"
        }
      },{
        "@id": "Model",
        "@type": "rdfs:Class",
        "rdfs:comment": "The set of specifications that describes data generated by a Resource and the commands that the Resource is able to process.",
        "rdfs:label": "Model",
        "rdfs:subClassOf":
        {
            "@id": "schema:Thing"
        }
      },{
        "@id": "Capability",
        "@type": "rdfs:Class",
        "rdfs:comment": "The specification that describe a particular measure or a specific command for a Resource.",
        "rdfs:label": "Capability",
        "rdfs:subClassOf":
        {
            "@id": "schema:Thing"
        }
      },{
        "@id": "hasCapability",
        "@type": "rdf:Property",
        "rdfs:comment": "Of a [[Model]], to indicate the supported capabilities.",
        "rdfs:label": "hasCapability",
        "schema:domainIncludes": [
          {
            "@id": "Model"
          }
        ],
        "schema:rangeIncludes": [
          {
            "@id": "schema:URL"
          },{
            "@id": "schema:Text"
          },{
            "@id": "Capability"
          }
        ]
      },{
          "@id": "hasModel",
          "@type": "rdf:Property",
          "rdfs:comment": "The relationship between a [[Resource]] and its [[Model]].",
          "rdfs:label": "hasModel",
          "schema:domainIncludes": {
            "@id": "Resource"
          },
          "schema:rangeIncludes": {
            "@id": "Model"
          }
      },{
          "@id": "modelCardinalityRestriction",
          "@type": "owl:Restriction",
          "owl:onClass": {
            "@id": "Model"
          },
          "owl:onProperty": {
            "@id": "hasModel"
          },
          "owl:qualifiedCardinality": {
            "@type": "xsd:nonNegativeInteger",
            "@value": "1"
          }
      },{
        "@id": "Cluster",
        "@type": "rdfs:Class",
        "rdfs:comment": "An aggreagation of Things",
        "rdfs:label": "Cluster",
        "rdfs:subClassOf": {
            "@id": "schema:Thing"
        }
      },{
        "@id": "DataChannel",
        "@type": "rdfs:Class",
        "rdfs:comment": "Generic Data Source",
        "rdfs:label": "DataChannel",
        "rdfs:subClassOf": {
            "@id": "schema:Thing"
        }
      },{
          "@id": "clusterOf",
          "@type": "rdf:Property",
          "rdfs:comment": "The relationship between a [[Cluster]] and its elements.",
          "rdfs:label": "clusterOf",
          "schema:domainIncludes": {
            "@id": "Cluster"
          },
          "schema:rangeIncludes": [{
            "@id": "schema:Thing"
          },{
            "@id": "Resource"
          },{
            "@id": "Cluster"
          }]
      }
    ]
}